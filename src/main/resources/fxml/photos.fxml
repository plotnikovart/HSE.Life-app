<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="340.0" prefWidth="356.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.front.PhotoController">
   <children>
      <TableView fx:id="table" editable="true" prefHeight="195.0" prefWidth="316.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <columns>
          <TableColumn fx:id="dayColumn" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="День" />
          <TableColumn fx:id="refColumn" onEditCommit="#changeRef" prefWidth="240.0" text="Ссылка на фотографию" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
